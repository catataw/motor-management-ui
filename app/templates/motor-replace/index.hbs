<ReplacedContainer as |replacedList isLoading pageIndex pageCount workerList pmList equipmentList options|>
  <ReplacedList @replacedList={{replacedList}} @isLoading={{isLoading}}
                @workerList={{workerList}} @pmList={{pmList}}
                @equipmentList={{equipmentList}}/>

  <PaginationPager @pageIndex={{pageIndex}} @pageCount={{pageCount}}
                   @gotoPage={{options.gotoPage}}/>
</ReplacedContainer>