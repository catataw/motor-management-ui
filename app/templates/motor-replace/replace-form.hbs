<ReplacedFormContainer as |userList pmList newForm equipmentList motorOnline storageMotors statuses options|>
  <ReplacedForm @userList={{userList}} @pmList={{pmList}} @newForm={{newForm}}
                @equipmentList={{equipmentList}}
                @getOnlineMotor={{options.getOnlineMotor}}
                @motorOnline={{motorOnline}}
                @statuses={{statuses}}
                @storageMotors={{storageMotors}}
                @searchMotorBySeriesNumber={{options.searchMotorBySeriesNumber}}
                @getSelectedPmEquipment={{options.getSelectedPmEquipment}}
                @saveNewReplaceForm={{options.saveNewReplaceForm}}/>
</ReplacedFormContainer>