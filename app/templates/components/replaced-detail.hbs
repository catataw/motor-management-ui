{{#if isLoading}}
  <div class="row"></div>
  <div class="row">
    <div class="col s12">
      <ContentPlaceholders as |P|>
        <P.heading/>
        <P.text @lines={{2}} />
      </ContentPlaceholders>
    </div>
  </div>
{{else}}
  <div class="row">
    <div class="col s12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title center">Replaced Form</span>
          <div class="row">
            <div class="col s4">
              <label>Location</label>
              {{input type="text" value=replacedMotor.location }}
            </div>
          </div>
        </div>
        <div class="card-action">
        </div>
      </div>
    </div>
  </div>

{{/if}}